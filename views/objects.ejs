<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Materias | We-Learning</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.5 -->
    <link rel="stylesheet" href="bootstrap/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="plugins/jvectormap/jquery-jvectormap-1.2.2.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="dist/css/skins/_all-skins.min.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
      <header class="main-header">
        <!-- Logo -->
        <a href="/" class="logo">
          <!-- mini logo for sidebar mini 50x50 pixels -->
          <span class="logo-mini">W-L</span>
          <!-- logo for regular state and mobile devices -->
          <span class="logo-lg"><img src="img/v-learning100.png"  alt="We-Learning" /></span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
          </a>
          <div class="navbar-custom-menu">
            <ul class="nav navbar-nav">

              <!-- User Account: style can be found in dropdown.less -->
              <li class="dropdown user user-menu">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                  <i class="fa fa-user"></i>
                  <span class="hidden-xs"><%= user.local.name %></span>
                </a>
                <ul class="dropdown-menu">
                  <!-- User image -->
                  <li class="user-header">
                    <% if (typeof user.local.photo === "undefined") { %>
                    <img src="/img/avatar.png" class="img-circle" alt="User Image">
                    <% } else { %>
                    <img src="<%= user.local.photo %>" class="img-circle" alt="User Image">
                    <% } %>
                    <p><%= user.local.name %></p>
                    <p>Rol: <%= user.local.role %></p>
                  </li>

                  <!-- Menu Footer-->
                  <li class="user-footer">
                    <div class="pull-left">
                      <a href="/profile" class="btn btn-default btn-flat">Perfil</a>
                    </div>
                    <div class="pull-right">
                      <a href="/logout" class="btn btn-default btn-flat">Cerrar Sesión</a>
                    </div>
                  </li>
                </ul>
              </li>
            </ul>
          </div>
        </nav>
      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
          <!-- Sidebar user panel -->
          <div class="user-panel">
            <div class="pull-left image">
              <% if (typeof user.local.photo === "undefined") { %>
              <img src="/img/avatar.png" class="img-circle" alt="User Image">
              <% } else { %>
              <img src="<%= user.local.photo %>" class="img-circle" alt="User Image">
              <% } %>
            </div>
            <div class="pull-left info">
              <p>Hola, <%= user.local.name %></p>
              <p><%= user.local.role %></p>
            </div>
          </div>

          <!-- sidebar menu: : style can be found in sidebar.less -->
          <ul class="sidebar-menu">
            <li class="header">MENÚ PRINCIPAL</li>
            <!--Panel de Control-->
            <li class="treeview">
              <a href="/dashboard">
                <i class="fa fa-dashboard"></i> <span>Panel de Control</span>
              </a>
            </li>
            <% if (user.local.role === "Administrador") { %>
            <!--Registro-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-person-add"></i> <span>Registro</span>
              </a>
              <ul class="treeview-menu">
                <li><a href="/users"><i class="fa fa-angle-double-right"></i> Usuarios</a></li>
                <li><a href="/objects"><i class="fa fa-angle-double-right"></i> Materias</a></li>
              </ul>
            </li>

            <!--Matriculas-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-bag"></i> <span>Matriculas</span>
              </a>
              <ul class="treeview-menu">
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Crear Estudiante</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Buscar Estudiante</a></li>
              </ul>
            </li>
            <% } %>
            <!--Pensum-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-compose"></i> <span>Pensum</span>
              </a>
              <ul class="treeview-menu">
                <% if (user.local.role === "Administrador") { %>
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Materia</a></li>
                <% } %>
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Logro</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Actividad</a></li>
              </ul>
            </li>

            <!--Control-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-stats-bars"></i><span>Control</span>
              </a>
              <ul class="treeview-menu">
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Asistencia</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Observador</a></li>
              </ul>
            </li>

            <!--Reportes-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-pie-graph"></i><span>Reportes</span>
              </a>
              <ul class="treeview-menu">
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Notas</a></li>
              </ul>
            </li>
            <% if (user.local.role === "Administrador") { %>
            <!--Configuración-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-gear-b"></i><span>Configuración</span>
              </a>
              <ul class="treeview-menu">
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Identidad Institucional</a></li>
              </ul>
            </li>
            <% } %>
          </ul>
        </section>
        <!-- /.sidebar -->
      </aside>
      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Administración de asignaturas
          </h1>
        </section>
        <!-- Main content -->
        <section class="content">
          <!-- Info boxes -->
          <div class="row">
            <div class="col-md-6 col-sm-4 col-xs-5">
              <div class="contains">
                <span class="btn bg-green fa fa-plus-circle fa-2x pull-left btn-lg btn-block" id="add-car"> Agregar </span>
              </div><!-- /.info-box -->
            </div>
         <!--  <div class="col-md-3 col-sm-4 col-xs-5">
              <div class="contains">
                <span class="btn btn-danger" id="btnCarRemove"><i class="fa fa-times-circle fa-5x pull-left"></i> Eliminar </span>
              </div>
            </div>
            <div class="clearfix visible-sm-block"></div>
            <div class="col-md-3 col-sm-4 col-xs-5">
              <div class="contains">
                <span class="btn bg-yellow" id="btnEditar"><i class="fa fa-pencil-square fa-5x pull-left"></i> Editar </span>
              </div>
            </div> -->
            <div class="col-md-6 col-sm-4 col-xs-5">
              <div class="contains">
                <span class="btn btn-primary fa fa-cog fa-2x pull-left btn-lg btn-block"> Ajustes </span>
              </div><!-- /.info-box -->
            </div><!-- /.col -->
          </div><!-- /.row -->
          <div class="box-body">
                  <div class="table-responsive" >
                    <table class="table no-margin" id="table-car">
                      <thead>
                        <tr>
                          <th class="text-align">ID Materia</th>
                          <th class="text-align">ID Docente</th>
                          <th class="text-align">Nombre Materia</th>
                          <th class="text-align">Fecha de Inicio</th>
                          <th class="text-align">Estado</th>
                          <th class="text-align">Descripción</th>
                          <th class="text-align">Editar</th>
                          <th class="text-align">Eliminar</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% if (object) { %>
                        <% object.forEach(function(obj) { %>
                        <tr class="<%= obj._id %>" style="text-align:center;">
                          <td><%= obj.idCurso %> </td>
                          <td><%= obj.idDocente %></td>
                          <td><%= obj.nombreMateria %></td>
                          <td><%= obj.fechaInicio %></td>
                          <td><%= obj.estado %></td>
                          <td><%= obj.descripcion %></td>
                          <td><favorite-star active class="star"></favorite-star></td>
                          <td>
                            <% if (user.local.role === "Administrador") { %>
                            <td>
                                  <button id="<%= obj._id %>" class="btnEditar btn btn-warning btn-block ">Editar <i class="fa fa-pencil"></i></button>
                                  <input type="hidden" value="<%= obj._id %>">
                            <% } else { %>
                                  <button id="<%= obj._id %>" class="disable btnEditar btn btn-warning btn-block">Editar <i class="fa fa-pencil"></i></button>
                                  <input type="hidden" value="<%= obj._id %>">
                            <% } %>
                            </td>
                            <td>
                            <% if (user.local.role === "Administrador") { %>
                                  <button id="<%= obj._id  %>" class="btnCarRemove btn btn-danger btn-block">Eliminar <i class="fa fa-trash"></i></button>
                                  <input type="hidden" value="<%= obj._id  %>">
                            <% } else { %>
                                  <button id="<%= obj._id  %>" class="disabled btnCarRemove btn btn-danger btn-block">Eliminar <i class="fa fa-trash"></i></button>
                                  <input type="hidden" value="<%= obj._id  %>">
                            <% } %>
                            </td>
                          </td>
                        </tr>
                        <% }); %>
                        <%} else {%>
                        <td colspan="" rowspan="" headers="">Para crear el objeto de click en el boton <span class="fa fa-plus-circle"></span></td>
                        <% } %>
                      </tbody>
                    </table>
                  </div><!-- /.table-responsive -->
                  <form class=" card form-car " enctype="multipart/form-data"  action="form" method="POST" >
                    <div id="section-1" class="card col-md-12" style="display: none;" >
                      <div>
                        <label for=""><h2>Ingrese la nueva materia</h2></label>
                      </div>
                      <section class="col-md-6 ">
                        <div class="form-group" id="idCurso">
                          <label for="bio">ID Materia:</label><br>
                          <input class="form-control" type="text" name="idCurso" value="" placeholder="0000" pattern="[0-9]{4}" required="required" >
                        </div>
                        <div class="form-group" id="idDocente">
                          <label for="bio">ID Docente:</label><br>
                          <input class="form-control" type="text" name="idDocente" value="" placeholder="0000" pattern="[0-9]{4}"required="required">
                        </div>
                        <div class="form-group" id="cursoList">
                          <label for="bio">Nombre Materia:</label><br>
                          <select class="form-control" type="brand" name="nombreMateria" id="career-list" required="required"></select>
                        </div>
                      </section>
                      <section class="col-md-6">
                        <div class="form-group" id="fechaInicio">
                          <label for="bio">Fecha de Inicio:</label><br>
                          <div class="col-md-4 col-sm-4 col-xs-5">
                            <select class="form-control" type="brand" name="diaCurso"  id="diaCurso"></select>
                          </div>
                          <div class="col-md-4 col-sm-4 col-xs-5">
                            <select class="form-control" type="brand" name="mesCurso" id="mesCurso"></select>
                          </div>
                          <div class="col-md-4 col-sm-4 col-xs-5">
                            <select class="form-control" type="brand" name="anoCurso" id="anoCurso"></select>
                          </div>
                        </div>
                        <div class="form-group" id="Estado">
                          <label for="bio">Estado:</label><br>
                          <select class="form-control" name="estado" id="estadoCurso">
                            <option value="Si">Activo</option>
                            <option value="No">Inactivo</option>
                          </select>
                        </div>
                        <div>
                          <label for="bio">Descripcion del asignatura:</label><br>
                          <input class="form-date btn btn-success btn-block" name="descripcion"  value="cargar archivo">
                        </div>
                      </section>
                      <a class="form-control btn btn-info" href="/dashboard">Ir a inicio</a>
                    </div>
                    <div id="form-car2" style="display:none;" >
                      <div  class="col-md-12 card"  >
                        <div class="form-group">
                          <label for="Descripcion de la materia">Cargar Archivo (.pdf)</label><br>
                          <input type="file" name="photos" required="required">
                          <p class="help-block">Materia.pdf</p>
                        </div>
                        <INPUT id="guardar"class="form-date btn btn-success btn-block" type="submit" value="guardar"></INPUT>
                        <INPUT class="Atras form-date btn btn-primary btn-block" type="submit" value="Atras"></INPUT>
                      </div>
                    </div>
                  </form>
          </div>
        </section>
        <!-- COPYRIGHT -->
        <footer class="main-footer">
          <strong>Copyright &copy; 2016 <a href="#">We-nnova</a>.</strong> Todos los derechos reservados.
          <div class="pull-right hidden-xs"><b>Versión</b> 1.0</div>
        </footer>
      </div><!-- ./wrapper -->

    <!--   Core JS Files   -->
    <script src="assets/dashboard/js/jquery-1.10.2.js" type="text/javascript"></script>
    <!--<script src="assets/dashboard/js/bootstrap.min.js" type="text/javascript"></script>-->

    <!--  Checkbox, Radio & Switch Plugins -->
    <script src="assets/dashboard/js/bootstrap-checkbox-radio-switch.js"></script>

    <!--  Charts Plugin -->
    <script src="assets/dashboard/js/chartist.min.js"></script>

    <!--  Notifications Plugin    -->
    <script src="assets/dashboard/js/bootstrap-notify.js"></script>

    <!--  Google Maps Plugin    -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>

    <!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
    <script src="assets/dashboard/js/light-bootstrap-dashboard.js"></script>

    <!-- Light Bootstrap Table DEMO methods, don't include it in your project! -->
    <script src="assets/dashboard/js/demo.js"></script>

    <!-- Script main-->
    <script src="json/main.js" type="text/javascript" charset="utf-8" ></script>
    <script type="text/javascript" src="json/jquery.autocomplete.js"></script>


    <!-- jQuery 2.1.4 -->
    <script src="plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
