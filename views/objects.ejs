        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

      <header class="main-header">
        <!-- Logo -->
        <a href="/" class="logo">
          <!-- mini logo for sidebar mini 50x50 pixels -->
          <span class="logo-mini">W-L</span>
          <!-- logo for regular state and mobile devices -->
          <span class="logo-lg"><img src="img/v-learning100.png"  alt="We-Learning" /></span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
          </a>
          <div class="navbar-custom-menu">
            <ul class="nav navbar-nav">
              
              <!-- User Account: style can be found in dropdown.less -->
              <li class="dropdown user user-menu">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                  <img src="<%= user.google.photo %>" class="user-image" alt="User Image">
                  <span class="hidden-xs">Google user: <%= user.google.name %></span>
                </a>
                <ul class="dropdown-menu">
                  <!-- User image -->
                  <li class="user-header">
                    <img src="<%= user.google.photo %>" class="img-circle" alt="User Image">
                    <p>Google user: <%= user.google.name %></p>
                  </li>

                  <!-- Menu Footer-->
                  <li class="user-footer">
                    <div class="pull-left">
                      <a href="/profile" class="btn btn-default btn-flat">Perfil</a>
                    </div>
                    <div class="pull-right">
                      <a href="/logout" class="btn btn-default btn-flat">Cerrar Sesión</a>
                    </div>
                  </li>
                </ul>
              </li>
            </ul>
          </div>
        </nav>
      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
          <!-- Sidebar user panel -->
          <div class="user-panel">
            <div class="pull-left image">
              <img src="<%= user.google.photo %>" class="img-circle" alt="User Image">
            </div>
            <div class="pull-left info">
              <p>Hola, Google user: <%= user.google.name %></p>
            </div>
          </div>
          
          <!-- sidebar menu: : style can be found in sidebar.less -->
          <ul class="sidebar-menu">
            
            <li class="header">MENÚ PRINCIPAL</li>

            <!--Panel de Control-->
            <li class="treeview">
              <a href="/dashboard">
                <i class="fa fa-dashboard"></i> <span>Panel de Control</span>
              </a>
            </li>

            {% if request.user.is_superuser %}
            <!--Registro-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-person-add"></i> <span>Registro</span> <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Usuarios</a></li>
                <li><a href="/objects"><i class="fa fa-angle-double-right"></i> Cursos</a></li>
              </ul>
            </li>

            <!--Matriculas-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-bag"></i> <span>Matriculas</span> <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Crear Estudiante</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Buscar Estudiante</a></li>
              </ul>
            </li>
            {%endif%}

            <!--Pensum-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-compose"></i> <span>Pensum</span> <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                {% if request.user.is_superuser %}
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Materia</a></li>
                {% if request.user.is_superuser %}
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Logro</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Actividad</a></li>
              </ul>
            </li>

            <!--Control-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-stats-bars"></i><span>Control</span><i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Asistencia</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Observador</a></li>
              </ul>
            </li>

            <!--Reportes-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-pie-graph"></i><span>Reportes</span><i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Notas</a></li>
              </ul>
            </li>

            {% if request.user.is_superuser %}
            <!--Configuración-->
            <li class="treeview">
              <a href="#">
                <i class="ion ion-gear-b"></i><span>Configuración</span><i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li><a href="#"><i class="fa fa-angle-double-right"></i> Identidad Institucional</a></li>
              </ul>
            </li>
            {%endif%}

          </ul>
        </section>
        <!-- /.sidebar -->
      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Administración de asignaturas
          </h1>
        </section>
        <!-- Main content -->
        <section class="content">
          <!-- Info boxes -->
          <div class="row">
            <div class="col-md-3 col-sm-4 col-xs-5">
              <div class="contains">
                <span class="btn bg-green" id="add-car"> <i class="fa fa-plus-circle fa-5x pull-left" href="#"></i> Agregar </span>                
              </div><!-- /.info-box -->
            </div>
            <div class="col-md-3 col-sm-4 col-xs-5">
              <div class="contains">
                <span class="btn btn-danger"><i class="fa fa-times-circle fa-5x pull-left" href="add-car"></i> Eliminar </span>                
              </div><!-- /.info-box -->
            </div><!-- /.col -->
                        <!-- fix for small devices only -->
            <div class="clearfix visible-sm-block"></div>
            <div class="col-md-3 col-sm-4 col-xs-5">
              <div class="contains">
                <span class="btn bg-yellow"><i class="fa fa-pencil-square fa-5x pull-left"></i> Editar </span>                
              </div><!-- /.info-box -->
            </div>
            <div class="col-md-3 col-sm-4 col-xs-5">
              <div class="contains">
                <span class="btn btn-primary"><i class="fa fa-cog fa-5x pull-left"></i> Ajustes </span>                
              </div><!-- /.info-box -->
            </div><!-- /.col -->
          </div><!-- /.row -->
          <div class="box-body">
                  <div class="table-responsive">
                    <table class="table no-margin">
                      <thead>
                        <tr>
                          <th class="text-align">ID Curso</th>
                          <th class="text-align">ID Docente</th>
                          <th class="text-align">Nombre Materia</th>
                          <th class="text-align">Fecha de Inicio</th>
                          <th class="text-align">Estado</th>
                          <th class="text-align">Descripción</th>
                          <th class="text-align">Seleccionar</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% if (object) { %>
                        <% object.forEach(function(obj) { %>
                        <tr class="<%= obj._id %>" style="text-align:center;">
                          <td><%= obj.idCurso %> </td>
                          <td><%= obj.idDocente %></td>
                          <td><%= obj.nombreMateria %></td>
                          <td><%= obj.fechaInicio %></td>
                          <td><%= obj.estado %></td>
                          <td><%= obj.descripcion %></td>
                          <td><favorite-star active class="star"></favorite-star></td>
                          <td>
                            <button id="<%= obj._id %>" class="btnCarRemove btn btn-primary btn-block ">Eliminar <i class="fa fa-trash"></i></button>
                            <input type="hidden" value="<%= obj._id %>">
                          </td>
                        </tr>
                        <% }); %>
                        <%} else {%>
                        <td colspan="" rowspan="" headers="">Para crear el objeto de click en el boton <span class="fa fa-plus-circle"></span></td>
                        <% } %>
                      </tbody>
                    </table>
                  </div><!-- /.table-responsive -->
                   <form class=" card form-car " enctype="multipart/form-data" style="display:none;" action="objects" method="POST" >
                                <div id="section-1" class="card col-md-12"  style="display:none;">
                                    <div>
                                        <label for=""><h2>Ingrese el nuevo curso</h2></label>
                                    </div>
                                    <section class="col-md-6 ">                                       
                                        <div class="form-group" id="idCurso">
                                            <label for="bio">ID Curso:</label><br>
                                            <input class="form-control" type="text" name="idCurso" value="" placeholder="000000">
                                       </div>
                                        <div class="form-group" id="idDocente">
                                            <label for="bio">ID Docente:</label><br>
                                            <input class="form-control" type="text" name="idDocente" value="" placeholder="000000">
                                       </div>
                                        <div class="form-group" id="cursoList">
                                            <label for="bio">Nombre curso:</label><br>
                                            <select class="form-control" type="brand" id="career-list"></select>
                                        </div>
                                    </section>
                                    <section class="col-md-6">
                                        <div class="form-group" id="fechaInicio">
                                          <label for="bio">Fecha de Inicio:</label><br>
                                          <div class="col-md-4 col-sm-4 col-xs-5">
                                            <select class="form-control" type="brand" id="diaCurso"></select>
                                          </div>
                                          <div class="col-md-4 col-sm-4 col-xs-5">
                                            <select class="form-control" type="brand"  id="mesCurso"></select>
                                          </div>
                                          <div class="col-md-4 col-sm-4 col-xs-5">
                                            <select class="form-control" type="brand"  id="ano-car">
                                            </select>
                                          </div>
                                        </div>
                                        <div class="form-group" id="Estado">
                                            <label for="bio">Estado:</label><br>
                                            <select class="form-control" name="estado"id="estadoCurso">
                                                <option value="Si">Activo</option>
                                                <option value="No">Inactivo</option>
                                            </select>
                                        </div>
                                        <input id="form-date" class="form-control btn btn-default" value ="cargar fotos">
                                    </section>
                                    <input id="form-date" class="form-control btn btn-default" value ="cargar fotos">
                                </div>
                                <div id="form-car2" style="display:none;">
                               <section  class="col-md-12 card" >
                                    <div class="form-group">
                                       <label for="foto_1">Adjuntar una foto</label>
                                        <input type="file" name="photos">
                                        <p class="help-block">Carro.png</p>
                                    </div>
                                    <div class="form-group">
                                       <label for="foto_2">Adjuntar una foto</label>
                                        <input type="file" name="photos">
                                        <p class="help-block">pefil-derecho.png</p>
                                    </div>
                                    <div class="form-group">
                                       <label for="foto_3">Adjuntar una foto</label>
                                        <input type="file" name="photos">
                                        <p class="help-block">pefil-inzquierdo.png</p>
                                    </div> 
                                    <INPUT class="form-control btn btn-default" type="submit" value="guardar">
                                </section>
                                </div>
                            </form>
                        </div>
                    </div>  
                </div>                    
            </div>
        </div>                     

        
        <!-- COPYRIGHT -->
        <footer class="main-footer">
          <strong>Copyright &copy; 2016 <a href="#">We-nnova</a>.</strong> Todos los derechos reservados.
          <div class="pull-right hidden-xs"><b>Versión</b> 1.0</div>
        </footer>

    </div><!-- ./wrapper -->

